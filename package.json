    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-outside-click-handler": "^1.3.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^4.2.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.6",
